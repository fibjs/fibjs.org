
/*! @brief 数据库记录对象 */
/*! @brief Database record object */
interface DBRow : object
{
    /*! @brief 对象数组可使用下标直接访问对象数据*/
    /*! @brief The element of the object array can be accessed by using subscript*/
    readonly Value operator[];

    /*! @brief 可使用字段名下标直接访问对象数据*/
    /*! @brief The subscript can be a string of the record name*/
    readonly Value operator[String];
};
